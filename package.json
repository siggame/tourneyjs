{
  "name": "@siggame/tourneyjs",
  "version": "0.0.3",
  "description": "Tournament Algorithms",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm index.js && npm run clean:src && npm run clean:test",
    "clean:src": "rimraf -rf src/*.js src/**/*.js",
    "clean:test": "rimraf -rf test/*.js test/**/*.js",
    "lint": "tslint --format verbose --project ./tsconfig.json",
    "pretest": "npm run build",
    "setup": "npm install",
    "postsetup": "npm run build",
    "test": "mocha --colors test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/tourneyjs.git"
  },
  "keywords": [
    "tournament",
    "triple-elimination",
    "elimination",
    "swiss-tournament",
    "swiss",
    "single-elimination"
  ],
  "author": "siggame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siggame/tourneyjs/issues"
  },
  "homepage": "https://github.com/siggame/tourneyjs#readme",
  "devDependencies": {
    "@siggame/colisee-tslint": "^1.0.3",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "@types/should": "^11.2.0",
    "mocha": "^3.5.2",
    "rimraf": "^2.6.1",
    "should": "^11.2.1"
  },
  "dependencies": {
    "typescript": "^2.5.2"
  }
}